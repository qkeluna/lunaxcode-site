{
  "info": {
    "name": "Lunaxcode API",
    "description": "FastAPI backend for Lunaxcode website",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    {
      "key": "baseUrl",
      "value": "https://lunaxcode-fastapi.vercel.app/api/v1",
      "type": "string"
    },
    {
      "key": "apiKey",
      "value": "your_api_key_here",
      "type": "string"
    }
  ],
  "item": [
    {
      "name": "Health",
      "item": [
        {
          "name": "Health Check",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/health",
              "host": ["{{baseUrl}}"],
              "path": ["health"]
            }
          }
        },
        {
          "name": "Database Health",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/health/db",
              "host": ["{{baseUrl}}"],
              "path": ["health", "db"]
            }
          }
        }
      ]
    },
    {
      "name": "Pricing",
      "item": [
        {
          "name": "Get All Pricing Plans",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/pricing",
              "host": ["{{baseUrl}}"],
              "path": ["pricing"]
            }
          }
        },
        {
          "name": "Get Pricing Plan by ID",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/pricing/landing_page",
              "host": ["{{baseUrl}}"],
              "path": ["pricing", "landing_page"]
            }
          }
        }
      ]
    },
    {
      "name": "Services",
      "item": [
        {
          "name": "Get All Services",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/services",
              "host": ["{{baseUrl}}"],
              "path": ["services"]
            }
          }
        },
        {
          "name": "Get Service by ID",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/services/landing_page",
              "host": ["{{baseUrl}}"],
              "path": ["services", "landing_page"]
            }
          }
        }
      ]
    },
    {
      "name": "Features",
      "item": [
        {
          "name": "Get All Features",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/features",
              "host": ["{{baseUrl}}"],
              "path": ["features"]
            }
          }
        }
      ]
    },
    {
      "name": "Add-ons",
      "item": [
        {
          "name": "Get All Add-ons",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/addons",
              "host": ["{{baseUrl}}"],
              "path": ["addons"]
            }
          }
        }
      ]
    },
    {
      "name": "Company",
      "item": [
        {
          "name": "Get Company Info",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/company",
              "host": ["{{baseUrl}}"],
              "path": ["company"]
            }
          }
        }
      ]
    },
    {
      "name": "Onboarding",
      "item": [
        {
          "name": "Get Questions for Landing Page",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/onboarding/questions/landing_page",
              "host": ["{{baseUrl}}"],
              "path": ["onboarding", "questions", "landing_page"]
            }
          }
        },
        {
          "name": "Get Questions for Basic Website",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/onboarding/questions/basic_website",
              "host": ["{{baseUrl}}"],
              "path": ["onboarding", "questions", "basic_website"]
            }
          }
        }
      ]
    },
    {
      "name": "Leads",
      "item": [
        {
          "name": "Create Lead (Public)",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"John Doe\",\n  \"email\": \"john@example.com\",\n  \"phone\": \"+63 912 345 6789\",\n  \"company\": \"Tech Startup Inc\",\n  \"service_type\": \"landing_page\",\n  \"budget_range\": \"5000-10000\",\n  \"timeline\": \"2 weeks\",\n  \"project_description\": \"Need a professional landing page for our SaaS product\",\n  \"answers\": {\n    \"target_audience\": \"B2B SaaS customers\",\n    \"key_features\": \"Lead capture, feature showcase, pricing table\"\n  },\n  \"source\": \"website\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/leads",
              "host": ["{{baseUrl}}"],
              "path": ["leads"]
            }
          }
        },
        {
          "name": "Get All Leads (Admin)",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "X-API-Key",
                "value": "{{apiKey}}",
                "type": "text"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/leads",
              "host": ["{{baseUrl}}"],
              "path": ["leads"]
            }
          }
        },
        {
          "name": "Get Lead by ID (Admin)",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "X-API-Key",
                "value": "{{apiKey}}",
                "type": "text"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/leads/1",
              "host": ["{{baseUrl}}"],
              "path": ["leads", "1"]
            }
          }
        }
      ]
    }
  ]
}

